def recurfibo(n):  
   if n <= 1:  
       return n  
   else:  
       return(recurfibo(n-1) + recurfibo(n-2))  
nterms = int(input("How many terms? "))  
if nterms<= 0:  
   print("Plese enter a positive integer")  
else:  
   print("Fibonacci sequence:")  
   for i in range(nterms):  
       print(recurfibo(i))
